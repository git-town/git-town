        "Main": {
          "type": "string"
        },
        "ObservedRegex": {
          "type": "string"
        },
        "Order": {
          "type": "string"
        },
        "PerennialRegex": {
          "type": "string"
        },
        "Perennials": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "UnknownType": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "type": "object",
